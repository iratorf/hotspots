<li>
	<div class="row">
		<div class="col-xs-2">
			<img src="http://www.hakkalabs.co/assets/placeholder.png" class="user-pic img-circle">
			<%= review.user().escape('email').match(/[^@]*/) %>
			<% date = new Date(review.escape('created_at')) %>
			<%= date.toLocaleDateString() %>
		</div>
		
		<div class="col-xs-10">
			<% for (var i = 0; i < review.get('score'); i++) { %>
				<span class="glyphicon glyphicon-star"></span>
			<% } %>
		</br>
			<%= review.escape("body") %>
		</div>
	</div>
	<hr>
</li>